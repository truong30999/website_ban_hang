@model int
@*
    For more information on enabling MVC for empty projects, visit http://go.microsoft.com/fwlink/?LinkID=397860
*@

<td style="width:150px">
    <div style="background-color:orange" class="btn-group border" role="group">
        <a type="button" class="btn btn-success" style="background-color:orange; border:1px solid orange" href="@Url.Action("Edit/"+Model).Replace("%2F","/")">
            <i class="fa fa-edit"></i>
        </a>
        <a type="button" class="btn btn-success"  href="@Url.Action("Details/"+Model).Replace("%2F","/")">
            <i class="fas fa-list-ul"></i>
        </a>
        <a type="button" class="btn btn-danger"  href="@Url.Action("Delete/"+Model).Replace("%2F","/")">
            <i class="fas fa-trash-alt"></i>
        </a>
    </div> 
</td>